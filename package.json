{
  "name": "photofinish",
  "version": "1.0.0",
  "license": "MIT",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --config=jest.config.json --runInBand",
    "test:update-snapshots": "jest --config=jest.config.json -u",
    "test:coverage": "jest --config=jest.config.json --coverage --runInBand",
    "test:ci": "npm run lint && npm run test:coverage",
    "lint": "eslint --format codeframe \"lib/**/*.ts\" \"test/**/*.ts\"",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\" index.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "percentile": "^1.3.0",
    "stats-lite": "^2.2.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.6",
    "@types/stats-lite": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.0.0",
    "typescript": "3.9.3",
    "zoology": "^1.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "statements": 85,
        "branches": 75,
        "functions": 90,
        "lines": 85
      }
    }
  },
  "homepage": "http://github.com/kibertoad/canned-enterprise-node",
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/canned-enterprise-node.git"
  },
  "keywords": [
    "benchmark",
    "performance",
    "measuring"
  ],
  "files": [
    "README.md",
    "LICENSE",
    "dist/*"
  ]
}
